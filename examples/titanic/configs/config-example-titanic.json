{
  "group_name": "example",
  "project": "titanic",
  "version": "1",
  "data_config": {
    "source": "csv",
    "local_name_source": "./data/titanic.csv",
    "table_name_source": "public.'TitanicExample'",
    "separation" : {
        "kind": "random", 
        "random_state": 1, 
        "test_train_proportion": 0.2,
        "period_column": ["AGE"]
    },
    "extra_columns": ["PASSENGERID" , "SURVIVED"],
    "data": {
        "targetslices": [{"column": "AGE", "type": "numerical", "slices": 5}]
    } 
  },
  "models_configs": [
    {
      "name": "first",
      "column_target": "SURVIVED",
      "objective": "poisson",
      "wrapper": "catboost",
      "relative_features": [],
      "features": [        
        {
          "name": "SEX",
          "enoding": "to_int",
          "default": "0",
          "replace": {
            "MALE": "1",
            "FEMALE": "2"
          }
        },
        {
          "name": "AGE",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "SIBSP",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        }
      ]
    },
    {
      "name": "second",
      "column_target": "SURVIVED",
      "wrapper": "catboost",
      "objective": "poisson",
      "relative_features": [],
      "features": [        
        {
          "name": "SIBSP",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "PARCH",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "FARE",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "AGE",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        }
      ]
    }   
  ]
}

