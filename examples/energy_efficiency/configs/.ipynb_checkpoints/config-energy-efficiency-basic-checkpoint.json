{
  "group_name": "example",
  "project": "energy-efficiency",
  "version": "1",
  "data_config": {
    "source": "csv",
    "local_name_source": "data/ENB2012_data.csv",
    "table_name_source": "public.'EnergyEfficiency'",
    "separation" : {
        "kind": "random", 
        "random_state": 1
    },
    "data": {
        "targetslices": [{"column": "Surface Area", "type": "numerical", "slices": 5}]
    } 
  },
  "models_configs": [
    {
      "name": "heating",
      "column_target": "Heating Load",
      "wrapper": "catboost",
      "relative_features": [],
      "features": [        
        {
          "name": "Relative Compactness",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        },
        {
          "name": "Roof Area",
          "default": 0,
          "replace": {"_TYPE_": "_NUM_"}
        }
      ]
    }
  ]
}

